{
  "version": "6.0",
  "knowledge": {
    "default_notebook": "ffa73340-e818-4bcc-9809-a58e62e52982",
    "auto_query": true
  },
  "routing": {
    "weights": {
      "scope": 25,
      "dependency": 20,
      "technical": 20,
      "risk": 15,
      "time": 10,
      "ui": 10
    },
    "thresholds": {
      "direct": 20,
      "planned": 45,
      "ralph": 70
    }
  },
  "checkpoint": {
    "auto_save": true,
    "atomic_writes": true,
    "backup_count": 3,
    "save_interval_minutes": 5,
    "max_history": 10
  },
  "cross_validation": {
    "enabled": true,
    "require_arbitration_on_disagreement": true,
    "min_confidence_for_auto_pass": "high"
  },
  "output": {
    "current_dir": ".skillpack/current",
    "history_dir": ".skillpack/history"
  },
  "mcp": {
    "timeout_seconds": 120,
    "max_retries": 1,
    "auto_fallback_to_cli": true
  },
  "cli": {
    "prefer_cli_over_mcp": true,
    "cli_timeout_seconds": 600,
    "codex_command": "codex",
    "gemini_command": "gemini",
    "auto_context": true,
    "max_context_files": 15,
    "max_lines_per_file": 800
  },
  "parallel": {
    "enabled": true,
    "max_concurrent_tasks": 3,
    "fallback_to_serial_on_failure": true
  },
  "adapter": {
    "auto_detect": true,
    "codex_min_version": "0.80.0",
    "gemini_min_version": "0.15.0",
    "codex_recommended": "0.89.0",
    "gemini_recommended": "0.25.0",
    "version_cache_ttl_seconds": 300,
    "show_upgrade_hints": true
  },
  "smart_routing": {
    "enabled": true,
    "codex_max_threshold_tokens": 100000,
    "gemini_flash_threshold": 5,
    "prefer_codex_for_code": true,
    "prefer_gemini_for_ui": true,
    "auto_model_upgrade": true
  },
  "tool_discovery": {
    "lazy_load": true,
    "cache_ttl_seconds": 300,
    "max_tools_per_request": 20,
    "preload_common_tools": true
  },
  "branch": {
    "enabled": true,
    "max_branches": 5,
    "auto_merge_threshold": 0.9,
    "preserve_history": true
  },
  "skill_system": {
    "enabled": true,
    "hot_reload": true,
    "user_skills_dir": "~/.skillpack/skills",
    "project_skills_dir": ".skillpack/skills",
    "debounce_ms": 500
  },
  "lsp": {
    "enabled": false,
    "auto_start": false,
    "supported_languages": ["typescript", "python", "go", "rust"],
    "timeout_seconds": 30
  }
}
